{
  "include": ["server/**/*.ts"],
  "exclude": [],
  "compilerOptions": {
    "module": "NodeNext",
    "target": "ES5",
    "lib": ["ES6", "DOM"],
    "declaration": false,
    "esModuleInterop": true,
    "removeComments": true,
    "strict": true,
    "rootDir": "server",
    "outDir": "dist/server",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./server/*"]
    }
  }
}
